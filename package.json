{
    "name": "augustus-api",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "db-up": "node ./node_modules/db-migrate/bin/db-migrate up --migrations-dir=./database/migrations --config=./database/database.json",
        "db-down": "node ./node_modules/db-migrate/bin/db-migrate reset --migrations-dir=./database/migrations --config=./database/database.json",
        "db-reset": "npm run db-down && npm run db-up",
        "rebuild-binary-deps": "docker exec backend npm rebuild --build-from-source bcrypt",
        "start": "nodemon -L --inspect=0.0.0.0:9229 src/index.js",
        "prod": "node src/index.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "deploy": "bash deploy.sh"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^3.0.6",
        "connect-pg-simple": "^5.0.0",
        "dataloader": "^1.4.0",
        "db-migrate": "^0.11.5",
        "db-migrate-pg": "^0.5.0",
        "dotenv": "^7.0.0",
        "elo-js": "^1.0.1",
        "express-jwt": "^5.3.1",
        "express-session": "^1.16.1",
        "faker": "^4.1.0",
        "graphql-yoga": "^1.18.3",
        "jwks-rsa": "^1.6.1",
        "lodash": "^4.17.11",
        "module-alias": "^2.2.0",
        "pg": "^7.10.0",
        "pg-hstore": "^2.3.2",
        "sequelize": "^5.3.1"
    },
    "devDependencies": {
        "jest": "^24.7.1",
        "nodemon": "^1.18.11"
    },
    "_moduleAliases": {
        "@api": "src/api",
        "@database": "database",
        "@guards": "src/guards",
        "@helpers": "src/helpers",
        "@loaders": "src/loaders",
        "@models": "src/models",
        "@repos": "src/repos",
        "@services": "src/services",
        "@tests": "src/tests",
        "@utils": "src/utils"
    }
}
